<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="MonoDevelop.References.proj" />
  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <OutputPath>..\..\bin</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Compile Update="MonoDevelop.LogMonitor.Gui\LogMonitorWidget.UI.cs">
      <DependentUpon>LogMonitorWidget.cs</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Properties\MonoDevelop.LogMonitor.addin.xml">
      <LogicalName>MonoDevelop.LogMonitor.addin.xml</LogicalName>
    </EmbeddedResource>
  </ItemGroup>
</Project>